#! /bin/bash

# build the string
for f in */*
do
  if [ -f $f ] ; then
    s+=$(cat $f | grep -B1 'Endpoint =')
  fi
done

# clean up the string out output
s=${s//\`/\"}  # replace all instances of tick mark with double quotes
s=${s//--/}    # remove the double dashes the cat put in sometimes
echo -e ${s//\/\/ /\\n}
